@using EntitiyLayer.Concrete
@model List<Team>

<section id="content-section-4">
    <div class="greennature-color-wrapper   " style="background-color: #ffffff; ">
        <div class="container">
            <div class="greennature-title-item" style="margin-bottom: 40px;">
                <div class="greennature-item-title-wrapper greennature-item  greennature-left-divider greennature-medium ">
                    <div class="greennature-item-title-container container">
                        <div class="greennature-item-title-head">
                            <h3 class="greennature-item-title greennature-skin-title greennature-skin-border">Meet The Team</h3>
                            <div class="clear"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clear"></div>
            <div class="greennature-personnel-item-wrapper">
                <div class="clear"></div>
                 @foreach (var item in Model)
                    {
                <div class="four columns">
                   
                        
                    
                    <div class="greennature-item greennature-personnel-item greennature-left plain-style">
                        <div class="greennature-ux greennature-personnel-ux">
                            <div class="personnel-item">
                                <div class="personnel-author-image greennature-skin-border"><img src="@item.ImageURL" alt="" width="500" height="297" /></div>
                                <div class="personnel-info">
                                    <div class="personnel-author greennature-skin-title">@item.PersonName</div>
                                    <div class="personnel-position greennature-skin-info">@item.Title</div>
                                </div>
                               
                                <div class="personnel-social"><a href="@item.FacebookURL" target="_self"><i class="greennature-icon fa fa-facebook" style="vertical-align: middle; color: #999999; font-size: 19px; "></i></a> <a href="@item.TwitterURL" target="_self"><i class="greennature-icon fa fa-twitter" style="vertical-align: middle; color: #999999; font-size: 19px; "></i></a> <a href="http://linkedin.com/" target="_self"><i class="greennature-icon fa fa-linkedin" style="vertical-align: middle; color: #999999; font-size: 19px; "></i></a> <a href="http://vimeo.com/" target="_self"><i class="greennature-icon fa fa-vimeo" style="vertical-align: middle; color: #999999; font-size: 19px; "></i></a> <a href="http://pinterest.com/" target="_self"><i class="greennature-icon fa fa-pinterest" style="vertical-align: middle; color: #999999; font-size: 19px; "></i></a></div>
                            </div>
                        </div>
                    </div>
                    

                </div>
                    }


                <div class="clear"></div>
                <div class="four columns">
                    <div class="greennature-item greennature-personnel-item greennature-left plain-style">
                        <div class="greennature-ux greennature-personnel-ux">
                            <div class="personnel-item">
                                <div class="personnel-author-image greennature-skin-border"></div>
                                <div class="personnel-info"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="clear"></div>
</section>
